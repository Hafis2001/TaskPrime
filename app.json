{
  "expo": {
    "name": "TaskPrime",
    "slug": "TaskPrime",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/taskprime.png", 
    "scheme": "taskprime",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/taskprime.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff6600"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIViewControllerBasedStatusBarAppearance": true,
        "UIStatusBarStyle": "UIStatusBarStyleLightContent"
      },
      "backgroundColor": "#ffffff"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/taskprime.png", 
        "backgroundColor": "#ffffff"
      },
      "package": "com.hafis2001.TaskPrime",
      "androidStatusBar": {
        "barStyle": "light-content",
        "backgroundColor": "#ff6600",
        "translucent": false
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/taskprime.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/taskprime.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ff6600",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6b485d04-15ec-42b3-ac1a-377f6a5a985b"
      }
    }
  }
}